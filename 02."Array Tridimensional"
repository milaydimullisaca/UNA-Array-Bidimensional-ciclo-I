#include <iostream>
using namespace std;
int main() {
    int matriz3D[3][3][3];
    cout << "Ingrese valores "<<endl;
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            for (int k = 0; k < 3; k++) {
                cin >> matriz3D[i][j][k];
            }
        }
    }
    cout << "Matriz tridimensional:" << endl;
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            for (int k = 0; k < 3; k++) {
                cout << matriz3D[i][j][k] << " ";
              }
            cout << endl;
        }
        cout << endl;
    }
    return 0;
}
